	<version>0.0.2-SNAPSHOT</version>
	<build>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-jar-plugin</artifactId>
				<!-- version>2.4</version-->
				<configuration>
				
					<excludes>
					<exclude>test/**/**</exclude>  
					</excludes>
				</configuration>
				
			</plugin>
		</plugins>
	</build>

